K,GAAI,EAAb;;AACA,QAAM,QAAO;AAAA,yEAAI,iBAAO,MAAP;AAAA;AAAA;AAAA;AAAA;AAAA;AACf,gBAAA,MAAK,GAAI,MAAT;AADe;AAAA,uBAEI,OAAO,CAAC,MAAD,CAFX;;AAAA;AAET,gBAAA,IAFS;AAGf,gBAAA,IAAI,CAAC,OAAL,GAAe,IAAI,CAAC,OAApB;AACA,gBAAA,IAAI,CAAC,YAAL,GAAoB,IAAI,CAAC,YAAzB;AACA,gBAAA,SAAS,CAAC,IAAV,GAAiB,CAAjB;;AALe;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAJ;;AAAA,sBAAP,QAAO;AAAA;AAAA;AAAA,OAAb;;AAQA,QAAM,MAAK;AAAA,0EAAI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACP,UAAU,CAAC,MAAD,oBAAa,IAAb,EADH;;AAAA;AAEb,gBAAA,IAAI,CAAC,OAAL,GAAe,EAAf;AACA,gBAAA,IAAI,CAAC,YAAL,GAAoB,EAApB;AACA,gBAAA,SAAS,CAAC,IAAV,GAAiB,CAAjB;;AAJa;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAJ;;AAAA,sBAAL,MAAK;AAAA;AAAA;AAAA,OAAX;;AAOA,QAAM,QAAO;AAAA,0EAAI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACT,UAAU,mBAAM,IAAN,EADD;;AAAA;AAEf,gBAAA,IAAI,CAAC,OAAL,GAAe,EAAf;AACA,gBAAA,IAAI,CAAC,YAAL,GAAoB,EAApB;;AAHe;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAJ;;AAAA,sBAAP,QAAO;AAAA;AAAA;AAAA,OAAb;;AAMA,QAAM,KAAI,GAAI,YAAY,EAA1B;AAEA,WAAO;AAAE,MAAA,IAAI,EAAJ,IAAF;AAAQ,MAAA,QAAQ,EAAR,QAAR;AAAkB,MAAA,KAAK,EAAL,KAAlB;AAAyB,MAAA,UAAU,EAAV,UAAzB;AAAqC,MAAA,SAAS,EAAT,SAArC;AAAgD,MAAA,QAAQ,EAAR,QAAhD;AAA0D,MAAA,MAAK,EAAL;AAA1D,KAAP;AACD,GAjCY;AAmCb,EAAA,OAAO,EAAE;AACP,IAAA,QADO,sBACI;AACT,WAAK,OAAL,CAAa,IAAb,CAAkB;AAAE,QAAA,IAAI,EAAE;AAAR,OAAlB;AACD;AAHM;AAnCI,CAAf","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <br /><br /><br /><br />\r\n    <form @submit.prevent=\"onSubmit\">\r\n\r\n      <div class=\"field\">\r\n        <label class=\"label\">Usuario</label>\r\n        <div class=\"control\">\r\n          <input class=\"input\" type=\"text\" \r\n          v-model=\"form.usuario\" placeholder=\"Ej. Alex Smith\" />\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"field\">\r\n        <label class=\"label\">Departamento</label>\r\n        <div class=\"control\">\r\n          <input\r\n            class=\"input\"\r\n            type=\"text\"\r\n            v-model=\"form.departamento\"\r\n            placeholder=\"Ej. Gerencia\"\r\n          />\r\n        </div>\r\n      </div>\r\n      <br />\r\n      <button v-if=\"condicion.updt==1\" type=\"submit\" class=\"btncrear button is-link mr-4\">Crear</button>\r\n    </form>\r\n    <button v-if=\"condicion.updt==2\" @click=\"update\" class=\"btnupdate button is-link mr-4\">Actualizar</button>\r\n    <button class=\"btnregre button is-danger\" @click.prevent=\"Regresar\">Regresar</button>\r\n    <div class=\"contbl container\">\r\n      <table class=\"table table-hover table-secondary is-fullwidth\">\r\n        <thead class=\"has-background-link-dark\">\r\n          <tr>\r\n            <th class=\"has-text-white-ter\" scope=\"col\">ID</th>\r\n            <th class=\"has-text-white-ter\" scope=\"col\">Usuarios</th>\r\n            <th class=\"has-text-white-ter\" scope=\"col\">Departamentos</th>\r\n            <th class=\"has-text-white-ter\" scope=\"col\">Editar</th>\r\n            <th class=\"has-text-white-ter\" scope=\"col\">Eliminar</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"{id,usuario,departamento} in users\" :key=\"id\">\r\n            <th scope=\"row\" class=\"text-secondary\">{{id}}</th>\r\n            <td>{{usuario}}</td>\r\n            <td>{{departamento}}</td>\r\n            <td><button class=\"button is-link is-rounded is-small is-light is-fullwidth\"\r\n            @click=\"loadData(id)\">Editar</button></td>\r\n            <td><button class=\"button is-danger is-rounded is-small \r\n            is-light is-fullwidth\" @click=\"deleteUser(id)\">Eliminar</button></td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { createUser,useLoadUsers,deleteUser,getUser,updateUser } from \"@/firebase\";\r\nimport { reactive } from \"vue\";\r\n\r\nexport default {\r\n  setup() {\r\n    const condicion = reactive({\r\n      updt: 1,\r\n    });\r\n\r\n    const form = reactive({ usuario: \"\", departamento: \"\" });\r\n\r\n    let idBono = \"\"\r\n    const loadData = async (bonoId) => {\r\n      idBono = bonoId\r\n      const bono = await getUser(bonoId)\r\n      form.usuario = bono.usuario\r\n      form.departamento = bono.departamento\r\n      condicion.updt = 2\r\n    }\r\n\r\n    const update = async () => {\r\n      await updateUser(idBono, {...form})\r\n      form.usuario = \"\";\r\n      form.departamento = \"\";\r\n      condicion.updt = 1\r\n    }\r\n\r\n    const onSubmit = async () => {\r\n      await createUser({ ...form });\r\n      form.usuario = \"\";\r\n      form.departamento = \"\";\r\n    };\r\n\r\n    const users = useLoadUsers()\r\n\r\n    return { form, onSubmit, users, deleteUser, condicion, loadData, update };\r\n  },\r\n\r\n  methods: {\r\n    Regresar() {\r\n      this.$router.push({ name: \"Bonos\" });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.contbl{\r\n    padding: 4rem 10rem 0 4rem;\r\n}\r\n.btncrear{\r\n  position: absolute;\r\n  margin-left: 1rem;\r\n}\r\n.btnupdate{\r\n  position: absolute;\r\n}\r\n.btnregre{\r\n  margin-left: 7rem;\r\n}\r\n</style>"],"sourceRoot":""}]}