/firebase\";\r\nimport { reactive } from \"vue\";\r\nimport moment from \"moment\";\r\n\r\nexport default {\r\n  setup() {\r\n    const filtro = reactive({\r\n      fechad: \"\",\r\n      fechah: \"\",\r\n      totalbono: 0,\r\n    });\r\n\r\n    const bonos = useLoadBonos();\r\n\r\n    const filtrar = () => {\r\n      console.log(bonos);\r\n      bonos.value.forEach((x) => {\r\n        if (\r\n          moment(filtro.fechad).isSameOrBefore(x.fecha) &&\r\n          moment(filtro.fechah).isSameOrAfter(x.fecha, \"day\")\r\n        ) {\r\n          console.log(x.bono);\r\n          filtro.totalbono += parseInt(x.bono);\r\n        }\r\n      });\r\n    };\r\n\r\n    return { filtro, filtrar };\r\n  },\r\n  methods:{\r\n      regresar(){\r\n          this.$router.push({ name: \"Bonos\" });\r\n      }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.content{\r\n    padding: 2rem 5rem 0rem 3rem;\r\n}\r\n.frm{\r\n    padding-top: 2rem;\r\n    padding-bottom: 2rem;\r\n}\r\n.btnr{\r\n    margin-left: 1rem;\r\n}\r\n.tbl{\r\n    padding-top: 5rem;\r\n    padding-bottom: 8rem;\r\n    padding-right: 28rem;\r\n    padding-left: 8rem;\r\n}\r\n</style>"],"sourceRoot":""}]}